{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_cv_search_fmpz_mat_is_hadamard" >&5
$as_echo "$ac_cv_search_fmpz_mat_is_hadamard" >&6; }
ac_res=$ac_cv_search_fmpz_mat_is_hadamard
if test "$ac_res" != no; then :
  test "$ac_res" = "none required" || LIBS="$ac_res $LIBS"

                        { $as_echo "$as_me:${as_lineno-$LINENO}: checking for library containing fmpz_poly_get_ZZX" >&5
$as_echo_n "checking for library containing fmpz_poly_get_ZZX... " >&6; }
