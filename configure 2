else
  sage_spkg_install_eclib=yes
fi


      # Extract the first word of "mwrank", so it can be a program name with args.
set dummy mwrank; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_path_MWRANK+:} false; then :
  $as_echo_n "(cached) " >&6
else
  case $MWRANK in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_MWRANK="$MWRANK" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_path_MWRANK="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  ;;
esac
fi
MWRANK=$ac_cv_path_MWRANK
if test -n "$MWRANK"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $MWRANK" >&5
$as_echo "$MWRANK" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


      if test -z "$ac_cv_path_MWRANK"; then :
  sage_spkg_install_eclib=yes
fi


fi


                if test "x$sage_spkg_install_eclib" = xno; then :

                                        sage_use_system_eclib=yes
                    { $as_echo "$as_me:${as_lineno-$LINENO}: will use system package and not install SPKG eclib" >&5
$as_echo "$as_me: will use system package and not install SPKG eclib" >&6;}

else

                    if test "x$sage_use_system_eclib" = xforce; then :

                        as_fn_error $? "given --with-system-eclib=force but no system package could be used" "$LINENO" 5

else

                        { $as_echo "$as_me:${as_lineno-$LINENO}: no suitable system package found for SPKG eclib" >&5
$as_echo "$as_me: no suitable system package found for SPKG eclib" >&6;}

fi

fi


fi

fi

fi



