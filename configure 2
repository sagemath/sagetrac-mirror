    LIBS="-l$ac_lib  $ac_func_search_save_LIBS"
  fi
  if ac_fn_cxx_try_link "$LINENO"; then :
  ac_cv_search_fmpz_mat_is_hadamard=$ac_res
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext
  if ${ac_cv_search_fmpz_mat_is_hadamard+:} false; then :
  break
fi
done
if ${ac_cv_search_fmpz_mat_is_hadamard+:} false; then :

