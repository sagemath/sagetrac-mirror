        sage_spkg_install_gmp=yes
    fi

                if test "x$sage_spkg_install_gmp" = xno; then :

                                        sage_use_system_gmp=yes
                    { $as_echo "$as_me:${as_lineno-$LINENO}: will use system package and not install SPKG gmp" >&5
