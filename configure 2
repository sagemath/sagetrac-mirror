else

                        { $as_echo "$as_me:${as_lineno-$LINENO}: no suitable system package found for SPKG sqlite" >&5
$as_echo "$as_me: no suitable system package found for SPKG sqlite" >&6;}

fi

fi


fi

fi

fi



