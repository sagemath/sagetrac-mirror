  LIBS="-larb $LIBS"

else
                 { $as_echo "$as_me:${as_lineno-$LINENO}: checking for acb_mat_eig_simple in -lflint-arb" >&5
$as_echo_n "checking for acb_mat_eig_simple in -lflint-arb... " >&6; }
if ${ac_cv_lib_flint_arb_acb_mat_eig_simple+:} false; then :
  $as_echo_n "(cached) " >&6
