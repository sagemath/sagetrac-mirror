set_bit_precision(42); /* test for versions >= v20190226 */
                        show_version();
                        return 0;

  ;
  return 0;
}
_ACEOF
if ac_fn_cxx_try_run "$LINENO"; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes; use eclib from the system" >&5
$as_echo "yes; use eclib from the system" >&6; }
else

            { $as_echo "$as_me:${as_lineno-$LINENO}: result: no; install eclib" >&5
$as_echo "no; install eclib" >&6; }
