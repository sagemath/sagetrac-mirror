if test "x$ac_cv_header_eclib_types_h" = xyes; then :

          { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether we can link and run a program using eclib" >&5
$as_echo_n "checking whether we can link and run a program using eclib... " >&6; }
          ECLIB_SAVED_LIBS="$LIBS"
          LIBS="$LIBS -lec"
          if test "$cross_compiling" = yes; then :
  { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "cannot run test program while cross compiling
See \`config.log' for more details" "$LINENO" 5; }
