  if test "$want_spkg" = if_installed; then :
  want_spkg=$is_installed
fi

  spkg_line=" \\$(printf '\n    ')database_odlyzko_zeta"
  case $is_installed-$want_spkg in #(
  *-yes) :
    as_fn_append SAGE_OPTIONAL_INSTALLED_PACKAGES "$spkg_line" ;; #(
  yes-no) :
    as_fn_append SAGE_OPTIONAL_CLEANED_PACKAGES "$spkg_line" ;; #(
  *) :
     ;;
esac
  SAGE_ENABLE_database_odlyzko_zeta=$want_spkg







