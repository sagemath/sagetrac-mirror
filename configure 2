    { $as_echo "$as_me:${as_lineno-$LINENO}: checking installing gmp/mpir? " >&5
$as_echo_n "checking installing gmp/mpir? ... " >&6; }
    if test x$sage_spkg_install_mpir = xyes -o x$sage_spkg_install_gmp = xyes; then
