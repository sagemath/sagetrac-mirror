# BEGIN SAGE_SPKG_CONFIGURE_BOOST

echo "-----------------------------------------------------------------------------" >& 6
echo "Checking whether SageMath should install SPKG boost..." >& 6
$as_echo "## ------------------------------------------------------ ##
## Checking whether SageMath should install SPKG boost... ##
## ------------------------------------------------------ ##" >& 5


# Check whether --with-system-boost was given.
if test "${with_system_boost+set}" = set; then :
  withval=$with_system_boost; sage_use_system_boost=$withval
else
  sage_use_system_boost=yes

fi


sage_spkg_name=boost

sage_spkg_install_boost=no





if test -n "`ls "${SAGE_SPKG_INST}/${sage_spkg_name}"-* 2>/dev/null`"; then :

    { $as_echo "$as_me:${as_lineno-$LINENO}: boost has already been installed by SageMath" >&5
$as_echo "$as_me: boost has already been installed by SageMath" >&6;}
    sage_spkg_install_boost=yes
    sage_use_system_boost=installed

fi

if ${sage_require_boost+:} false; then :

else
  sage_require_boost=yes
fi


if test "x$sage_spkg_install_boost" = xno; then :

    if test "x$sage_require_boost" = xno; then :

        { $as_echo "$as_me:${as_lineno-$LINENO}: SPKG boost is not required on this system" >&5
$as_echo "$as_me: SPKG boost is not required on this system" >&6;}

else

                                if test "x$sage_use_system_boost" = xno; then :

                        sage_spkg_install_boost=yes

else






    { $as_echo "$as_me:${as_lineno-$LINENO}: checking whether any of boost_cropped is installed as or will be installed as SPKG" >&5
$as_echo_n "checking whether any of boost_cropped is installed as or will be installed as SPKG... " >&6; }
    if test x = y  -o x$sage_spkg_install_boost_cropped = xyes; then :

        { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes; install boost as well" >&5
$as_echo "yes; install boost as well" >&6; }
        sage_spkg_install_boost=yes
else

        { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }

  if test "$cross_compiling" = yes; then :
  { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "cannot run test program while cross compiling
See \`config.log' for more details" "$LINENO" 5; }
else
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */
  #include <boost/program_options/errors.hpp>

int
main ()
{

      boost::program_options::error err("Error message");
      return 0;

  ;
  return 0;
}
_ACEOF
if ac_fn_cxx_try_run "$LINENO"; then :

else
  sage_spkg_install_boost=yes
fi
rm -f core *.core core.conftest.* gmon.out bb.out conftest$ac_exeext \
  conftest.$ac_objext conftest.beam conftest.$ac_ext
fi



fi


                if test "x$sage_spkg_install_boost" = xno; then :

                                        sage_use_system_boost=yes
                    { $as_echo "$as_me:${as_lineno-$LINENO}: will use system package and not install SPKG boost" >&5
$as_echo "$as_me: will use system package and not install SPKG boost" >&6;}

else

                    if test "x$sage_use_system_boost" = xforce; then :

                        as_fn_error $? "given --with-system-boost=force but no system package could be used" "$LINENO" 5

else

                        { $as_echo "$as_me:${as_lineno-$LINENO}: no suitable system package found for SPKG boost" >&5
$as_echo "$as_me: no suitable system package found for SPKG boost" >&6;}

fi

fi


fi

fi

fi



